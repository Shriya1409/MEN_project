<!doctype html>
<html lang="en">
  <head>
    <%- include('common/head.ejs')-%>

    <title>GCE - Goa College of Engineering</title>
  </head>
  <body>
    
    <%- include('common/header.ejs')-%>

    
      <section>
        <div class="container">

          <div id="carouselExampleIndicators" class="carousel slide" data-ride="carousel"
           style="
           position:absolute;
           top:200px;
          text-align:center;
          align-items: center;
          left:260px;
          margin-top:50px;
           "
          
          >
            <ol class="carousel-indicators">
              <li data-target="#carouselExampleIndicators" data-slide-to="0" class="active"></li>
              <li data-target="#carouselExampleIndicators" data-slide-to="1"></li>
              <li data-target="#carouselExampleIndicators" data-slide-to="2"></li>
            </ol>
            <div class="carousel-inner"  style="width: 1000px; height:500px; text-align: center; align-items: center; padding:10px 10px; background-color: rgb(49, 38, 98);">
              <div class="carousel-item active" style="width: 1000px; margin-right:20px;" >
                <img style="width:1000px;" src="../backend/homepage-slider/home_fssai.jpg">
              </div>
              <div class="carousel-item"  style="width: 1000px;">
                <img style="width:1000px;" src="../backend/homepage-slider/home_nss.jpg">
              </div>
              <div class="carousel-item"  style="width: 1000px;">
                <img style="width:1000px;" src="../backend/homepage-slider/home_welcome.jpg" alt="Third slide">
              </div>
            </div>
            <a class="carousel-control-prev" href="#carouselExampleIndicators" role="button" data-slide="prev">
              <span class="carousel-control-prev-icon" aria-hidden="true"></span>
              <span class="sr-only">Previous</span>
            </a>
            <a class="carousel-control-next" href="#carouselExampleIndicators" role="button" data-slide="next">
              <span class="carousel-control-next-icon" aria-hidden="true"></span>
              <span class="sr-only">Next</span>
            </a>
          </div>

        </div>


      </section>

    <%- include('common/footer.ejs')-%>

  </body>
</html>


