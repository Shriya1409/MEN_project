                                // Set the autoplay interval in milliseconds (e.g., 3000ms = 3 seconds)
                                const autoplayInterval = 3000;
                                
                                // Function to simulate a button click on the next button
                                function goToNextSlide() {
                                  const nextButton = carousel.querySelector('.next-p');
