<%- include('common/head.ejs')-%>

   <title>Admin Panel - Edit Page</title>
   </head>
   <body>
    <%- include('common/header.ejs')-%>

      <div class="container-fluid bg-side-nav">
         <div class="row">
            <div class="col-lg-2 sidebaare">

                <%- include('common/sidebar.ejs')-%>

            </div>
            <div class="col-lg-10">

              <div class="dashboard-content-area">



                 <!-- put your contente -->
                 <h1 class="mb-2 mt-2">Edit Page</h1>
    
                 <form method="post" action="/result/edit-result/<%=x.pageUrll%>?_method=put" enctype="multipart/form-data">
             
                   <div class="mb-3 mt-3 row g-9 align-items-center">  
                     <div class="col col-lg-2"><label class="form-label">URL</label></div>
                     <div class="col"><input type="text" name ="page_Urll" class="form-control" value="<%=x.pageUrll%>" placeholder="Page Url" required></div>
                   </div>

                   <div class="mb-3 mt-3 row g-9 align-items-center">  
                      <div class="col col-lg-2"><label class="form-label">Nav Display Text</label></div>
                      <div class="col"><input type="text" name ="page_Nav_Textt" class="form-control" value="<%=x.pageNavTextt%>" placeholder="Navigation Display Text"></div>
                    </div>

                    <div class="mb-3 mt-3 row g-9 align-items-center">  
                      <div class="col col-lg-2"><label class="form-label">Page Title</label></div>
                      <div class="col"><input type="text" name ="page_Titlee" class="form-control" value="<%=x.pageTitlee%>" placeholder="Page Title" required></div>
                    </div>
             
                   <div class="mb-3 mt-3 row g-9 align-items-center">           
                       <div class="col col-lg-2"><label class="form-label">Meta Description</label></div>
                       <div class="col"><input type="text" name ="page_Meta_Descriptionn" class="form-control" value="<%=x.pageMetaDescriptionn%>" placeholder="Page Meta Description" ></div>
                   </div>
             
                   <div class="mb-3 mt-3 row g-9 align-items-center">
                     <div class="col col-lg-2"><label class="form-label">Meta Keyword</label></div>
                     <div class="col"><input type="text" name ="page_Meta_Keywordd" class="form-control" value="<%=x.pageMetaKeywordd%>" placeholder="Page Meta Keyword" ></div>
                   </div>
                   
                   <div class="mb-3 mt-3 row g-9 align-items-center">
                     <div class="col col-lg-2"><label class="form-label">h1</label></div>
                     <div class="col"><input type="text" name ="page_Headingg" class="form-control" value="<%=x.pageHeadingg%>" placeholder="Page Heading" ></div>
                   </div>
                   
                   <div class="mb-3 mt-3 row g-9 align-items-center"> 
                     <div class="col col-lg-2"><label class="form-label">Photo</label></div>
                     <div class="col"><input type="file" name="page_Photoo" class="form-control" ></div>
                   </div>
                   
                   <div class="mb-3 mt-3 row g-9 align-items-center">
                     <div class="col col-lg-2"><label class="form-label">Page Details</label></div>
                     <div class="col"><textarea rows="10" class="editor form-control Page_Details" id="editor" name="page_Detailss" placeholder="Page Details"><%=x.pageDetailss%></textarea></div>
                   </div>
             
                   <div class="mb-5 mt-3 align-items-center text-center"> 
                        <button type="submit" class="btn btn-primary">Submit</button>
                   </div>
               </form>

            <!-- end your content here -->




             </div>

            </div>
         </div>
      </div>
<%- include('common/footer.ejs')-%>
      