<%- include('common/head.ejs')-%>

   <title>Admin Panel - Edit Result</title>
   </head>
   <body>
    <%- include('common/header.ejs')-%>

      <div class="container-fluid bg-side-nav">
         <div class="row">
            <div class="col-lg-2 sidebaare" style="height:700px; background-color: black;">

                <%- include('common/sidebar.ejs')-%>

            </div>
            <div class="col-lg-10">

              <div class="dashboard-content-area">



                 <!-- put your contente -->
                 <h1 class="mb-2 mt-2">Edit Result</h1>
    
                 <form method="post" action="/result/edit-result/<%=x.course%>?_method=put" enctype="multipart/form-data">
             
                   <div class="mb-3 mt-3 row g-9 align-items-center">  
                     <div class="col col-lg-2"><label class="form-label">Course</label></div>
                     <div class="col"><input type="text" name ="course" class="form-control" value="<%=x.course%>" placeholder="Course" required></div>
                   </div>

                   <div class="mb-3 mt-3 row g-9 align-items-center">  
                      <div class="col col-lg-2"><label class="form-label">Semester</label></div>
                      <div class="col"><input type="text" name ="semester" class="form-control" value="<%=x.semester%>" placeholder="Year"></div>
                    </div>

                    <div class="mb-3 mt-3 row g-9 align-items-center">  
                      <div class="col col-lg-2"><label class="form-label">RC</label></div>
                      <div class="col"><input type="text" name ="rc" class="form-control" value="<%=x.rc%>" placeholder="RC Batch" required></div>
                    </div>
             
             
                                     
                   <div class="mb-3 mt-3 row g-9 align-items-center"> 
                     <div class="col col-lg-2"><label class="form-label">Result Pdf</label></div>
                     <div class="col"><input type="file" name="resultpdf" class="form-control" ></div>
                   </div>
                   
                   <div class="mb-3 mt-3 row g-9 align-items-center">
                     <div class="col col-lg-2"><label class="form-label">Page Details</label></div>
                     <div class="col"><textarea rows="10" class="editor form-control Page_Details" id="editor" name="page_Details" placeholder="Page Details"><%=x.pageDetails%></textarea></div>
                   </div>
             
                   <div class="mb-5 mt-3 align-items-center text-center"> 
                        <button type="submit" class="btn btn-primary">Submit</button>
                   </div>
               </form>

            <!-- end your content here -->




             </div>

            </div>
         </div>
      </div>
<%- include('common/footer.ejs')-%>
      